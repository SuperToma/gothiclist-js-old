{
  "name": "gothiclist",
  "version": "0.0.1",
  "description": "Site Gothiclist.com",
  "private": true,
  "engines": {
    "node": ">= 4.2.x",
    "npm": ">= 3.10.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "dev:debug": "nodemon --inspect boot.js",
    "dev:server": "nodemon boot.js",
    "dev:client": "webpack --watch",
    "dev:test": "npm test -- --watch",
    "lint": "standard --verbose | snazzy",
    "postinstall": "npm run build",
    "start": "node boot.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:cov": "npm test -- --coverage"
  },
  "babel": {
    "presets": [
      "latest-minimal",
      "stage-3"
    ]
  },
  "standard": {
    "env": {
      "node": true,
      "jest": true
    },
    "parser": "babel-eslint",
    "ignore": [
      "/doc/"
    ]
  },
  "keywords": [
    "node",
    "express",
    "connect",
    "passport",
    "social",
    "jade",
    "webpack",
    "mocha",
    "tests"
  ],
  "author": "Thomas HAMOU <Thomas.HAMOU@supinfo.com>",
  "license": "No-License (see LICENSE file)",
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-latest-minimal": "^1.1.2",
    "babel-preset-stage-3": "^6.22.0",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.6",
    "colors": "^1.0.3",
    "connect-flash": "^0.1.1",
    "cookie-session": "^2.0.0-beta.1",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "csurf": "^1.9.0",
    "errorhandler": "^1.5.0",
    "express": "^4.15.2",
    "file-loader": "^0.10.1",
    "hjs-webpack": "^8.4.3",
    "isomorphic-fetch": "^2.2.1",
    "jade": "^1.8.1",
    "jade-loader": "^0.8.0",
    "jquery": "^3.2.1",
    "method-override": "^2.3.8",
    "mongoose": "^4.9.2",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-twitter": "^1.0.2",
    "postcss-loader": "^1.3.3",
    "select2": "^3.5.2-browserify",
    "socket.io": "^1.7.3",
    "style-loader": "^0.16.1",
    "stylus": "^0.54.2",
    "stylus-loader": "^3.0.1",
    "underscore": "^1.7.0",
    "unfluff": "^1.0.0",
    "url-loader": "^0.5.8",
    "webpack": "1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "bson-objectid": "^1.1.5",
    "chai": "^3.5.0",
    "faker": "^4.1.0",
    "groc": "^0.8.0",
    "jest": "^19.0.2",
    "nock": "^9.0.10",
    "nodemon": "^1.9.1",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "snazzy": "^6.0.0",
    "standard": "^9.0.2",
    "supertest": "^3.0.0"
  }
}
